<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.archstudio.myxgen.brick">
      <brick
            class="org.archstudio.schematron.core.SchematronMyxComponent"
            id="org.archstudio.schematron.core.SchematronMyxComponent"
            name="Schematron Impl">
         <interface
               class="org.archstudio.archlight.IArchlightTool"
               delegate="brick"
               direction="in"
               domain="top"
               getter="true"
               id="tools"
               name="tools"
               single="true">
         </interface>
         <interface
               class="org.archstudio.testadt.IArchlightTestADT"
               delegate="variable"
               direction="out"
               domain="top"
               getter="true"
               id="tests"
               name="tests"
               single="true">
         </interface>
         <interface
               class="org.archstudio.xarchadt.IXArchADT"
               delegate="variable"
               direction="out"
               domain="top"
               getter="true"
               id="xarch"
               name="xarch"
               single="true">
         </interface>
      </brick>
   </extension>

</plugin>
