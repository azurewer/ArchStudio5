<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.archstudio.myxgen.brick">
      <brick
            abstract="true"
            class="org.archstudio.editors.AbstractArchstudioEditorMyxComponent"
            id="org.archstudio.editors.AbstractArchstudioEditorMyxComponent"
            name="Abstract ArchStudio Editor Component Impl">
         <interface
               class="org.archstudio.resources.IResources"
               delegate="variable"
               direction="out"
               getter="true"
               id="resources"
               name="resources"
               single="true">
         </interface>
         <interface
               class="org.archstudio.editormanager.IEditorManager"
               delegate="variable"
               direction="out"
               getter="true"
               id="editorManager"
               name="editorManager"
               single="true">
         </interface>
         <interface
               class="org.archstudio.editors.IFocusEditorListener"
               delegate="events"
               direction="in"
               getter="true"
               id="focusEditorEvents"
               name="focusEditorEvents"
               single="true">
         </interface>
         <interface
               class="org.archstudio.filemanager.IFileManager"
               delegate="variable"
               direction="out"
               getter="true"
               id="fileManager"
               name="fileManager"
               single="true">
         </interface>
         <interface
               class="org.archstudio.filemanager.IFileManagerListener"
               delegate="events"
               direction="in"
               getter="true"
               id="fileManagerEvents"
               name="fileManagerEvents"
               single="true">
         </interface>
         <interface
               class="org.archstudio.xarchadt.IXArchADT"
               delegate="variable"
               direction="out"
               getter="true"
               id="xarch"
               name="xarch"
               single="true">
         </interface>
         <interface
               class="org.archstudio.xarchadt.IXArchADTFileListener"
               delegate="events"
               direction="in"
               getter="true"
               id="fileEvents"
               name="fileEvents"
               single="true">
         </interface>
         <interface
               class="org.archstudio.xarchadt.IXArchADTModelListener"
               delegate="events"
               direction="in"
               getter="true"
               id="modelEvents"
               name="modelEvents"
               single="true">
         </interface>
         <interface
               class="org.archstudio.launcher.ILaunchable"
               delegate="brick"
               direction="in"
               getter="true"
               id="launcher"
               name="launcher"
               single="true">
         </interface>
      </brick>
   </extension>

</plugin>
