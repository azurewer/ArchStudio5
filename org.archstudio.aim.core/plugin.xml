<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.archstudio.myxgen.brick">
      <brick
            class="org.archstudio.aim.core.AIMMyxComponent"
            id="org.archstudio.aim.core.AIMMyxComponent"
            name="AIM Myx Component Impl">
         <interface
               class="org.archstudio.aim.IAIM"
               delegate="variable"
               direction="in"
               domain="bottom"
               getter="true"
               id="aim"
               name="aim"
               single="true">
         </interface>
         <interface
               class="org.archstudio.myx.fw.IMyxRuntime"
               delegate="variable"
               direction="out"
               domain="top"
               getter="true"
               id="myxRuntime"
               name="myxRuntime"
               single="true">
         </interface>
         <interface
               class="org.archstudio.xarchadt.IXArchADT"
               delegate="variable"
               direction="out"
               domain="top"
               getter="true"
               id="xarch"
               name="xarch"
               single="true">
         </interface>
      </brick>
   </extension>

</plugin>
