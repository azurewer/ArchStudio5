<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.archstudio.myxgen.brick">
      <brick
            class="org.archstudio.editormanager.core.EditorManagerMyxComponent"
            id="org.archstudio.editormanager.core.EditorManagerMyxComponent"
            name="org.archstudio.editormanager.core.EditorManagerMyxComponent">
         <interface
               class="org.archstudio.editormanager.IEditorManager"
               delegate="variable"
               direction="in"
               getter="true"
               id="editorManager"
               name="editorManager"
               single="true">
         </interface>
         <interface
               class="org.archstudio.editors.IFocusEditorListener"
               delegate="events"
               direction="out"
               getter="true"
               id="focuseEditorEvents"
               name="focuseEditorEvents"
               single="true">
         </interface>
         <interface
               class="org.eclipse.jface.preference.IPreferenceStore"
               delegate="variable"
               direction="out"
               getter="true"
               id="preferences"
               name="preferences"
               single="true">
         </interface>
      </brick>
      <brick
            class="org.archstudio.editormanager.core.EditorPrefsMyxComponent"
            id="org.archstudio.editormanager.core.EditorPrefsMyxComponent"
            name="org.archstudio.editormanager.core.EditorPrefsMyxComponent">
         <interface
               class="org.archstudio.editormanager.IEditorManager"
               delegate="variable"
               direction="in"
               getter="true"
               id="editorManager"
               name="editorManager"
               single="true">
         </interface>
         <interface
               class="org.eclipse.jface.preference.IPreferenceStore"
               delegate="variable"
               direction="in"
               getter="true"
               id="preferences"
               name="preferences"
               single="true">
         </interface>
      </brick>
   </extension>

</plugin>
