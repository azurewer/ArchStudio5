<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.archstudio.archipelago2.outlines">
      <Path
            name="Structures">
      </Path>
      <ContentProvider
            class="org.archstudio.archipelago2.ext.structures.outline.StructuresContentProvider">
      </ContentProvider>
      <MenuContributor
            class="org.archstudio.archipelago2.ext.structures.outline.StructuresMenuContributor">
      </MenuContributor>
   </extension>
   <extension
         point="org.archstudio.archipelago2.editors">
      <Editor
            class="org.archstudio.archipelago2.ext.structures.StructureEditor"
            name="Structure">
      </Editor>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.archstudio.archipelago2.Archipelago2Preferences"
            class="org.archstudio.archipelago2.ext.structures.StructurePreferences"
            id="org.archstudio.archipelago2.ext.structures.StructurePreferences"
            name="Structure">
      </page>
   </extension>

</plugin>
